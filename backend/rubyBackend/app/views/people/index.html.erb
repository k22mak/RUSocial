<h1>Listing people</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Password</th>
    <th>Geox</th>
    <th>Geoy</th>
    <th>Sessionstatus</th>
    <th>Messages</th>
    <th>Timeconnected</th>
    <th>Lastmodified</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @people.each do |person| %>
  <tr>
    <td><%= person.username %></td>
    <td><%= person.password %></td>
    <td><%= person.geoX %></td>
    <td><%= person.geoY %></td>
    <td><%= person.sessionStatus %></td>
    <td><%= person.messages %></td>
    <td><%= person.timeConnected %></td>
    <td><%= person.lastModified %></td>
    <td><%= link_to 'Show', person %></td>
    <td><%= link_to 'Edit', edit_person_path(person) %></td>
    <td><%= link_to 'Destroy', person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

